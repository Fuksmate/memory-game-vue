QAAM,QAAO,GAAI,SAAX,QAAW,CAAC,OAAD,EAAa;AAC5B,MAAA,QAAQ,CAAC,KAAT,CAAe,OAAO,CAAC,QAAvB,EAAiC,SAAjC,GAA6C,IAA7C;;AACA,UAAI,aAAa,CAAC,KAAd,CAAoB,CAApB,CAAJ,EAA4B;AAC1B,YAAI,aAAa,CAAC,KAAd,CAAoB,CAApB,EAAuB,QAAvB,KAAoC,OAAO,CAAC,QAAhD,EAA0D;AACxD;AACF,SAFA,MAEO;AACL,UAAA,aAAa,CAAC,KAAd,CAAoB,CAApB,IAAyB,OAAzB;AACF;AACF,OANA,MAMO;AACL,QAAA,aAAa,CAAC,KAAd,CAAoB,CAApB,IAAyB,OAAzB;AACF;AACD,KAXD;;AAaA,IAAA,KAAK,CACH,aADG,EAEH,UAAC,cAAD,EAAoB;AAClB,UAAI,cAAc,CAAC,MAAf,KAA0B,CAA9B,EAAiC;AAC/B,YAAM,SAAQ,GAAI,cAAc,CAAC,CAAD,CAAhC;AACA,YAAM,UAAS,GAAI,cAAc,CAAC,CAAD,CAAjC;AACA,YAAM,WAAU,GAAI,SAAS,CAAC,SAAV,KAAwB,UAAU,CAAC,SAAvD;;AACA,YAAI,WAAJ,EAAiB;AACf,UAAA,UAAU,CAAC,YAAM;AACf,YAAA,QAAQ,CAAC,KAAT,CAAe,SAAS,CAAC,QAAzB,EAAmC,SAAnC,GAA+C,IAA/C;AACA,YAAA,QAAQ,CAAC,KAAT,CAAe,UAAU,CAAC,QAA1B,EAAoC,SAApC,GAAgD,IAAhD;AACD,WAHS,EAGP,GAHO,CAAV;AAIF,SALA,MAKO;AACL,UAAA,UAAU,CAAC,YAAM;AACf,YAAA,QAAQ,CAAC,KAAT,CAAe,SAAS,CAAC,QAAzB,EAAmC,SAAnC,GAA+C,KAA/C;AACA,YAAA,QAAQ,CAAC,KAAT,CAAe,UAAU,CAAC,QAA1B,EAAoC,SAApC,GAAgD,KAAhD;AACD,WAHS,EAGP,GAHO,CAAV;AAIF;;AAEA,QAAA,aAAa,CAAC,KAAd,CAAoB,MAApB,GAA6B,CAA7B;AACF;AACD,KArBE,EAsBH;AAAE,MAAA,IAAI,EAAE;AAAR,KAtBG,CAAL;AAyBA,IAAA,WAAW;AAEX,WAAO;AACL,MAAA,QAAQ,EAAR,QADK;AAEL,MAAA,QAAQ,EAAR,QAFK;AAGL,MAAA,aAAa,EAAb,aAHK;AAIL,MAAA,SAAS,EAAT,SAJK;AAKL,MAAA,WAAW,EAAX,WALK;AAML,MAAA,YAAY,EAAZ,YANK;AAOL,MAAA,MAAM,EAAN;AAPK,KAAP;AASD;AAnEY,CAAf","sourcesContent":["<template>\r\n  <section class=\"game-board\">\r\n    <Card\r\n      v-for=\"card in cardList\"\r\n      :key=\"card.position\"\r\n      :value=\"card.value\"\r\n      :isVisible=\"card.isVisible\"\r\n      :isMatched=\"card.isMatched\"\r\n      :position=\"card.position\"\r\n      @click=\"flipCard\"\r\n    />\r\n  </section>\r\n  <h2>{{ status }}</h2>\r\n  <button @click=\"restartGame\" class=\"reset-button\">Reset Gry</button>\r\n</template>\r\n\r\n<script>\r\nimport Card from \"./Card\";\r\nimport { ref, watch } from \"vue\";\r\nimport deck from \"@/data/deck.json\";\r\nimport createDeck from \"@/features/createDeck.js\";\r\nimport createGame from \"@/features/createGame.js\";\r\n\r\nexport default {\r\n  name: \"GameBoard\",\r\n  components: {\r\n    Card,\r\n  },\r\n  setup() {\r\n    const { cardList } = createDeck(deck);\r\n    const { restartGame, startGame, matchesFound, status } =\r\n      createGame(cardList);\r\n    const userSelection = ref([]);\r\n\r\n    cardList.value = cardList.value.map((card, index) => {\r\n      return {\r\n        ...card,\r\n        position: index,\r\n      };\r\n    });\r\n\r\n    const flipCard = (payload) => {\r\n      cardList.value[payload.position].isVisible = true;\r\n      if (userSelection.value[0]) {\r\n        if (userSelection.value[0].position === payload.position) {\r\n          return;\r\n        } else {\r\n          userSelection.value[1] = payload;\r\n        }\r\n      } else {\r\n        userSelection.value[0] = payload;\r\n      }\r\n    };\r\n\r\n    watch(\r\n      userSelection,\r\n      (selectionValue) => {\r\n        if (selectionValue.length === 2) {\r\n          const cardFirst = selectionValue[0];\r\n          const cardSecond = selectionValue[1];\r\n          const isisMatched = cardFirst.faceValue === cardSecond.faceValue;\r\n          if (isisMatched) {\r\n            setTimeout(() => {\r\n              cardList.value[cardFirst.position].isMatched = true;\r\n              cardList.value[cardSecond.position].isMatched = true;\r\n            }, 500);\r\n          } else {\r\n            setTimeout(() => {\r\n              cardList.value[cardFirst.position].isVisible = false;\r\n              cardList.value[cardSecond.position].isVisible = false;\r\n            }, 500);\r\n          }\r\n\r\n          userSelection.value.length = 0;\r\n        }\r\n      },\r\n      { deep: true }\r\n    );\r\n\r\n    restartGame();\r\n\r\n    return {\r\n      cardList,\r\n      flipCard,\r\n      userSelection,\r\n      startGame,\r\n      restartGame,\r\n      matchesFound,\r\n      status,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.game-board {\r\n  display: grid;\r\n  grid-template-columns: repeat(4, 120px);\r\n  grid-template-rows: repeat(4, 120px);\r\n  grid-column-gap: 5vw;\r\n  grid-row-gap: 4vh;\r\n  justify-content: center;\r\n}\r\n.reset-button {\r\n  box-shadow: inset 0px 1px 3px 0px #91b8b3;\r\n  background: linear-gradient(to bottom, #e8e8e8 5%, #6c7c7c 100%);\r\n  background-color: #e8e8e8;\r\n  border-radius: 5px;\r\n  border: 1px solid #566963;\r\n  display: inline-block;\r\n  cursor: pointer;\r\n  color: #ffffff;\r\n  font-family: Arial;\r\n  font-size: 22px;\r\n  font-weight: bold;\r\n  padding: 11px 23px;\r\n  width: 300px;\r\n  text-decoration: none;\r\n  text-shadow: 0px -1px 0px #2b665e;\r\n}\r\n.full-screen-button {\r\n  margin: 20px;\r\n  color: #1ccece;\r\n  background-color: #1b1e20;\r\n  font-size: 3rem;\r\n  vertical-align: middle;\r\n  box-sizing: border-box;\r\n  display: inline-block;\r\n  border: 0.1em solid black;\r\n  width: 1em;\r\n  height: 1em;\r\n  position: relative;\r\n}\r\n.full-screen-button:before,\r\n.full-screen-button:after {\r\n  content: \"\";\r\n  background: #1b1e20;\r\n  position: absolute;\r\n}\r\n.full-screen-button:before {\r\n  width: 0.333em;\r\n  height: 1em;\r\n  left: 0.233em;\r\n  top: -0.1em;\r\n}\r\n.full-screen-button:after {\r\n  width: 1em;\r\n  height: 0.333em;\r\n  top: 0.233em;\r\n  left: -0.1em;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}